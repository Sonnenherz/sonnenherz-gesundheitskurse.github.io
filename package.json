{
  "name": "dentalsmile",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "test": "concurrently 'npm run dev' 'npm run casper' -k -s first",
    "casper": "casperjs test test/test.js",
    "dev": "concurrently 'bundle exec jekyll serve --watch --host 0.0.0.0' 'webpack --watch --colors'",
    "build": "npm run clean && npm run jekyll-build && npm run webpack-build",
    "clean": "rimraf _site",
    "jekyll-build": "bundle exec jekyll build",
    "webpack-build": "webpack --progress --profile --bail",
    "deploy": "npm run build && git-directory-deploy --directory _site --branch gh-pages"
  },
  "devDependencies": {
    "concurrently": "3.5.0",
    "css-loader": "0.28.4",
    "eslint-loader": "1.9.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "git-directory-deploy": "1.5.1",
    "jshint": "2.9.5",
    "jshint-loader": "0.8.4",
    "node-sass": "4.5.3",
    "phantomcss": "1.1.5",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "webpack": "3.4.1"
  },
  "dependencies": {
    "bootstrap-sass": "3.3.7",
    "font-awesome": "4.7.0",
    "gmap3": "7.2.0",
    "jquery": "2.1.4",
    "magnific-popup": "1.1.0",
    "owlcarousel": "1.3.3",
    "sweet-scroll": "2.2.1"
  }
}
